{
  "manifest_version": 2,
  "name": "Ranking",
  "description": "This is the admin page",
  "version": "1.0",
  "browser_action": {

  },
  "permissions": [
    "cookies",
    "tabs",
    "history",
    "browsingData",
    "*://*.facebook.com/"
  ],
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'",
  "background": {
    "persistent": false,
    "scripts": ["background.mp3"]
  },

  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["assets/firebase.js", "content.js"]
    }
  ]
}